{
  "name": "ky-nice-ui",
  "version": "1.0.0",
  "description": "react ui components base on antd",
  "main": "index.js",
  "scripts": {
    "new": "node ./scripts/index.js",
    "remove": "node ./scripts/delete.js",
    "watch:es": "rollup --config rollup.config.js --environment MODULE:es --watch",
    "watch:cjs": "rollup --config rollup.config.js --environment MODULE:cjs --watch",
    "watch": "npm run watch:es",
    "website": "cd website && npm run dev",
    "build": "rollup --config rollup.config.js",
    "build:prod": "rollup --config rollup.config.js --environment NODE_ENV:production"
  },
  "author": "rgy",
  "license": "ISC",
  "dependencies": {
    "antd": "~3.26.0",
    "axios": "^0.18.0",
    "highlight.js": "^9.15.10",
    "marked": "^0.7.0",
    "mobx": "^4.8.0",
    "mobx-react": "^5.2.8",
    "prop-types": "^15.6.2",
    "rc-queue-anim": "^1.6.11",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-quill": "^1.3.3",
    "react-redux": "^7.1.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.9.0",
    "redux": "^4.0.4",
    "uniqueid": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "rollup": "^1.31.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0"
  },
  "files": [
    "src",
    "README.md",
    "VERSION.md"
  ]
}
