  "author": "rgy",
  "license": "ISC",
  "dependencies": {
    "antd": "~3.26.0",
    "axios": "^0.18.0",
    "highlight.js": "^9.15.10",
    "lodash.debounce": "^4.0.8",
    "marked": "^0.7.0",
    "mobx": "^4.8.0",
    "mobx-react": "^5.2.8",
    "prop-types": "^15.6.2",
    "rc-queue-anim": "^1.6.11",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-quill": "^1.3.3",
    "react-redux": "^7.1.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.9.0",
    "redux": "^4.0.4",
    "uniqueid": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
