{
  "presets": [
    "env",
    "es2015",
    "stage-0",
    "react"
  ],
  "plugins": [
    [
      "import", {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ],
    "transform-runtime",
    "transform-decorators-legacy",
    "react-hot-loader/babel"
  ]
}